{
  "name": "katsefet",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run tslint&& npm run build-ts",
    "release": "standard-version",
    "start": "tsc && cross-env NODE_ENV=dev node dist/server.js",
    "test": "npm run tslint&& npm run run_tests",
    "run_tests": "cross-env NODE_ENV=testing mocha --opts ./mocha.opts src/**/*.spec.ts src/specs/spec.runner.ts",
    "prod": "tsc && cross-env NODE_ENV=prod node dist/server.js",
    "test-file": "cross-env NODE_ENV=testing mocha --opts ./mocha.opts src/file/**/*.spec.ts",
    "test-router": "cross-env NODE_ENV=testing&& tsc && mocha --exit dist/**/**/*-spec.js --timeout 20000",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
